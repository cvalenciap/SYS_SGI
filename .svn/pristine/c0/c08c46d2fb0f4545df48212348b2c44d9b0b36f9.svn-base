button.accordion {
    background-color: #222d32;
    color: white;
    cursor: pointer;
    padding-right: 10px;
    width: 100%;
    border: none;
    text-align: left;
    outline: none;
    font-size: 15.1px;
    transition: 0.4s;
}

    button.accordion:after {
        content: '\002B';
        color: white;
        font-weight: bold;
        float: right;
        font-size: 17px;
    }

    button.accordion.active:after {
        content: "\2212";
    }

