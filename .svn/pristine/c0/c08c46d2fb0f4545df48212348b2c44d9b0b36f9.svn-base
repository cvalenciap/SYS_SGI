/*Cabecera*/
.main-header {
    z-index: 1010;
}

.sidebar-toggle-corpac, .sidebar-toggle-corpac:hover {
    color: black; /**cambiar color de letra a titulo*/
    float: left;
    background-color: transparent;
    background-image: none;
    padding: 13px 0px 0px 15px;
    font-family: 'Source Sans Pro',sans-serif;
    font-size: 18.5px;
    text-transform: uppercase;
    font-weight: bold;
}

.navbar-custom-menu-corpac {
    background-color: black; /*cambiar color de fondo a div de usuario*/
}

.skin-blue-light .main-header .navbar {
    background-color: #f9fafc;
}

    .skin-blue-light .main-header .navbar .sidebar-toggle {
        color: #337ab7;
    }

@media (max-width: 767px) {
    .sidebar-toggle-corpac {
        color: gray !important;
    }

    .skin-blue-light .main-header .navbar {
        background-color: #337ab7 !important;
    }

        .skin-blue-light .main-header .navbar .sidebar-toggle {
            color: #fff !important;
            background-color: green;
        }
}

.form-control-feedback {
    padding-top: 5px;
